---
import Layout from "../../layouts/Layout.astro";

---

<Layout seo={{ title: "Ex-Chairmans" }}>
    <div class="container mx-auto px-4 py-8">
        <h1 class="text-3xl font-bold text-center mb-8">Ex-Chairmen of AJK Public Service Commission</h1>
        <div class="overflow-x-auto">
            <table class="min-w-full bg-white border border-gray-300">
                <thead>
                    <tr class="bg-yellow-500">
                        <th class="px-6 py-3 border-b text-left">No.</th>
                        <th class="px-6 py-3 border-b text-left">Name</th>
                        <th class="px-6 py-3 border-b text-left">From</th>
                        <th class="px-6 py-3 border-b text-left">To</th>
                    </tr>
                </thead>
                <tbody>
                    {[
                        { no: 1, name: "Mr. Justice Khawaja Mohammad Sharif", from: "17-04-1967", to: "17-03-1975" },
                        { no: 2, name: "Mr. Justice Ch.Niaz Ahmed", from: "18-03-1975", to: "07-08-1976" },
                        { no: 3, name: "Mr. Prof. M.A Aziz", from: "08-08-1976", to: "27-10-1977" },
                        { no: 4, name: "Mr. Justice Khawaja Sharif", from: "06-02-1978", to: "04-02-1983" },
                        { no: 5, name: "Mr. Justice Shar Zaman Ch.", from: "17-02-1983", to: "16-02-1986" },
                        { no: 6, name: "Mr. Justice Malik Mohammad Aslam", from: "01-03-1986", to: "28-02-1989" },
                        { no: 7, name: "Mr. Justice Qazi Abdul Ghafoor", from: "11-03-1989", to: "10-3-1992" },
                        { no: 8, name: "Mr. Maj. (R) Tariq Nazami", from: "01-04-1992", to: "30-09-1995" },
                        { no: 9, name: "Mr. Mohammad Yousaf Awan", from: "17-10-1995", to: "02-09-1998" },
                        { no: 10, name: "Mr. Bostan Ch.", from: "04-09-1998", to: "03-09-2001" },
                        { no: 11, name: "Mr. Justice (R)Shair Zaman Ch.", from: "26-10-2001", to: "23-09-2002" },
                        { no: 12, name: "Mr. Brig.(R) Saeed Akhter", from: "24-09-2002", to: "12-03-2009" },
                        { no: 13, name: "Mr. Sardar Mohammad Latif Khan", from: "16-06-2009", to: "15-06-2012" },
                        { no: 14, name: "Syed Mumtaz Hussain Naqvi", from: "21-11-2012", to: "16-12-2013" },
                        { no: 15, name: "Justice(R) Khawaja Shahaad Ahmad", from: "20-08-2014", to: "15-12-2016" },
                        { no: 16, name: "Lt. General (r) Mohsin Kamal", from: "27-12-2016", to: "26-12-2019" },
                        { no: 17, name: "Air Marshal Masood Akhtar (Retd)", from: "05-05-2020", to: "04-05-2023" },
                    ].map((chairman) => (
                        <tr class="hover:bg-gray-50">
                            <td class="px-6 py-4 border-b">{chairman.no}</td>
                            <td class="px-6 py-4 border-b">{chairman.name}</td>
                            <td class="px-6 py-4 border-b">{chairman.from}</td>
                            <td class="px-6 py-4 border-b">{chairman.to}</td>
                        </tr>
                    ))}
                </tbody>
            </table>
        </div>
    </div>
    
</Layout>