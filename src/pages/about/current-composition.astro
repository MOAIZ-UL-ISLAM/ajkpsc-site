---
import Layout from "../../layouts/Layout.astro";

---

<Layout seo={{ title: "Curent composition of commision " }}>
<div class="container mx-auto px-4 py-8">
    <h2 class="text-2xl font-bold mb-6">Current Composition of Commission</h2>
    <table class="w-full border-collapse border border-gray-300">
        <thead>
            <tr class="bg-gray-100">
                <th class="border border-gray-300 px-4 py-2">Sr. No.</th>
                <th class="border border-gray-300 px-4 py-2">Name</th>
                <th class="border border-gray-300 px-4 py-2">Designation</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td class="border border-gray-300 px-4 py-2">1</td>
                <td class="border border-gray-300 px-4 py-2">Lt.General (r) Hidayat ur Rehman</td>
                <td class="border border-gray-300 px-4 py-2">Chairman</td>
            </tr>
            <tr>
                <td class="border border-gray-300 px-4 py-2">2</td>
                <td class="border border-gray-300 px-4 py-2">Aijaz Hussain Lone</td>
                <td class="border border-gray-300 px-4 py-2">Member</td>
            </tr>
            <tr>
                <td class="border border-gray-300 px-4 py-2">3</td>
                <td class="border border-gray-300 px-4 py-2">Hafiz Muhammad Ibrahim Aziz</td>
                <td class="border border-gray-300 px-4 py-2">Member</td>
            </tr>
            <tr>
                <td class="border border-gray-300 px-4 py-2">4</td>
                <td class="border border-gray-300 px-4 py-2">Saeed Bashir Butt</td>
                <td class="border border-gray-300 px-4 py-2">Member</td>
            </tr>
        </tbody>
    </table>
</div>
</Layout>